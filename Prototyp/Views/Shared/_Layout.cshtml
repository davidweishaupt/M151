<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Prototype Compager</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navColor">

        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="/Event/Index">
                    <img src="~/Content/Pictures/0.PNG">
                </a>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="/StartNumber/Index">
                            <img src="~/Content/Pictures/1.PNG">
                        </a>
                    </li>
                    <li>
                        <a href="/Discipline/Index">
                            <img src="~/Content/Pictures/2.PNG">
                        </a>
                    </li>
                    <li>
                        <a href="/Club/Index">
                            <img src="~/Content/Pictures/3.PNG">
                        </a>
                    </li>
                    <li>
                        <a href="/Athlete/Index">
                            <img src="~/Content/Pictures/4.PNG">
                        </a>
                    </li>
                    <li>
                        <a href="/Login/Index">
                            <img src="~/Content/Pictures/LogOut.PNG" width="120", height="66">
                        </a>
                        
                    </li>
                </ul>

                





            </div>

        </div>
    </div>
    

    <div class="container body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
